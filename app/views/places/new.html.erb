<h1>Share Your Escape</h1>
<%= form_for Place.new do |f| %>
<div class="places_form">
		<p><%= flash[:placeerror] if flash[:placeerror] %></p>
		<%= f.label :name, value: 'Place' %>
		<%= f.text_field :name %>

		<%= f.label :country, value: "Country" %>
		<%= f.text_field :country %>


		<%= f.submit class: 'submit_button', value: 'Share' %>
</div>
	<% end %>
